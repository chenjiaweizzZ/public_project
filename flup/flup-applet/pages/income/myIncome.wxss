.my-income {
  background: #F6F6F6;
  width: 100vw;
  height: 100vh;
  background-color: #F4F5F9;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.navigatorbar {
  position: relative;
  top: 0;
  width: 750rpx;
  height: 172rpx;
  flex-shrink: 0;
  background: #FFFFFF;
}

.navigatorbar-back {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  left: 40rpx;
  bottom: 20rpx;
}

.conversation-title {
  position: absolute;
  width: 350rpx;
  height: 88rpx;
  line-height: 56rpx;
  font-size: 36rpx;
  color: #000000;
  bottom: 0;
  left: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  overflow: hidden;
}

.time-bored {
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  padding: 10px 16px;
  align-items: center;
  height: 40px;
}

.time-bored .time-bored-left {
  font-size: 15px;
  font-weight: 500;
  line-height: 19px;
  letter-spacing: 0.2px;
  color: #111111;
}


.time-bored .time-bored-right {
  font-size: 16px;
  font-weight: 600;
  line-height: 19px;
  text-align: right;
  letter-spacing: 0px;
  color: #111111;
}

.time-bored .time-bored-right {
  font-size: 14px;
  font-weight: normal;
  line-height: 19px;
  letter-spacing: 0.2px;
  color: #666666;
}

.income-list {
  overflow: scroll;
}

.income-item {
  height: 50px;
  background: #FFFFFF;
  box-shadow: inset 0px -0.5px 0px 0px #D9D9D9;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100vw;
  overflow: hidden;
}

.income-item-left {
  margin-left: 14px;
}

.income-item-middle {
  flex: 3;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: flex;
  align-items: center;
  margin-left: 14px;
}

.income-item-right {
  flex: 1;
  display: flex;
  justify-content: center;
}