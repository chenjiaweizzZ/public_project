.item {
  margin-top: 20px;
}

.item .key {
  font-size: 17px;
  font-weight: 700;
  line-height: 20px;
  letter-spacing: 0.2px;
  color: #111111;
  margin-bottom: 20px;
}

.item .value {
  padding-left: 20px;
}

.image-list {
  display: flex;
  flex-wrap: wrap;
}

.image-list image {
  width: 100px;
  height: 100px;
  margin-right: 5px;
  margin-bottom: 5px;
}

.image-message {
  width: 100%;
  max-height: 300rpx;
  height: 300rpx;
  border-radius: 10px 10px 10px 10px;
}