.recommended-package {
  width: 100vw;
  height: 100vh;
  background-color: #F4F5F9;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.navigatorbar {
  position: relative;
  top: 0;
  width: 750rpx;
  height: 172rpx;
  flex-shrink: 0;
  background: #FFFFFF;
}

.navigatorbar-back {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  left: 40rpx;
  bottom: 20rpx;
}

.conversation-title {
  position: absolute;
  width: 350rpx;
  height: 88rpx;
  line-height: 56rpx;
  font-size: 36rpx;
  color: #000000;
  bottom: 0;
  left: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
  overflow: hidden;
}

.mains {
  flex: 1;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.equity-information {
  width: 100%;
  height: 500rpx;
  background: linear-gradient(180deg, #39B95F 43%, rgba(150, 255, 181, 0) 100%);
  padding: 30rpx 24rpx 44rpx;
  box-sizing: border-box;
}

.information-name {
  color: #FFFFFF;
  font-size: 40rpx;
}

.equity-project-list {
  margin-top: 32rpx;
  height: 296rpx;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 20rpx 0px rgba(0, 0, 0, 0.2);
  border-radius: 20rpx;
  padding: 22rpx 32rpx;
  box-sizing: border-box;
}

.equity-project-list {
  display: flex;
  flex-direction: column;
}

.equity-project-list .header {
  display: flex;
  justify-content: space-between;
}

.equity-project-list .header .name {
  font-size: 28rpx;
  color: #111111;
}

.equity-project-list .header .image image {
  width: 30rpx;
  height: 30rpx;
}

.project-list {
  flex: 1;
  margin-top: 16rpx;
  overflow-y: auto;
  /* padding-right: 16rpx; */
}

.project-list .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 6rpx 0;
}

.project-list .item .title {
  display: flex;
  font-size: 28rpx;
  color: #111111;
  align-items: center;
}

.project-list .item .title .number {
  margin-right: 26rpx;
}

.project-list .item .title .number .text {
  color: #FF5500;
}

.project-list .item .write-off {
  font-size: 24rpx;
  color: #00982D;
}

.operation-list {
  position: relative;
  /* flex: 1; */
  margin: 0 22rpx;
  overflow-y: auto;
}

.ceshi {
  position: absolute;
  content: "";
  width: 2rpx;
  background-color: #C6C9CE;
  left: 12rpx;
  z-index: 0;
}

.operation-list .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  margin-right: 32rpx;
  margin: 0;
  padding: 22rpx;
  margin-bottom: 20rpx;
  background: #FFFFFF;
  border-radius: 8px;
  box-sizing: border-box;
}

.operation-list .items {
  display: flex;
  align-items: center;
}

.operation-list .items image {
  flex-shrink: 0;
  width: 24rpx;
  height: 24rpx;
  margin-right: 32rpx;
  z-index: 10;
}

.operation-list .item image {
  width: 18rpx;
  height: 18rpx;
}

.operation-list .item .item-list {
  display: flex;
  margin-bottom: 16rpx;
}

.operation-list .item .item-list .write-off {
  flex-shrink: 0;
  width: 60rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: #425F7E;
  border: 2rpx solid #425F7E;
  border-radius: 4rpx;
  margin-right: 22rpx;
}

.operation-list .item .item-list .adjustment {
  flex-shrink: 0;
  width: 60rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: #0079FD;
  border: 2rpx solid #0079FD;
  border-radius: 4rpx;
  margin-right: 22rpx;
}

.operation-list .item .item-list .content {
  font-size: 30rpx;
  color: #111111;
  word-break: break-all;
}

.operation-list .item .remark {
  font-size: 28rpx;
  color: #408755;
  margin-bottom: 16rpx;
}

.operation-list .item .time {
  font-size: 28rpx;
  color: #A0A5AD;
}

.operation-list .item .time text {
  margin-right: 20rpx;
}

.popup-content {
  box-sizing: border-box;
  padding: 0 14px;
}

.popup-content .title {
  margin-top: 12px;
  text-align: center;
  font-size: 20px;
  font-weight: 700;
  line-height: 22px;
  letter-spacing: 0.2px;
  color: #111111;
}

.content-footer {
  box-sizing: border-box;
  display: flex;
  height: 58px;
  padding: 8px 14px;
  background: #FFFFFF;
  width: 100%;
  margin-top: 22px;
}

.content-footer .close {
  flex: 1;
  height: 42px;
  line-height: 42px;
  text-align: center;
  box-sizing: border-box;
  border: 1px solid #CDCDCD;
  border-radius: 100px;
  color: #666666;
  margin-right: 13px;
  font-size: 16px;
}

.content-footer .confirm {
  flex: 1;
  height: 42px;
  line-height: 42px;
  text-align: center;
  box-sizing: border-box;
  background: linear-gradient(123deg, #FF9035 28%, #FF7B35 73%);
  border-radius: 100px;
  color: #FFFFFF;
  font-size: 16px;
}

.popup-content {
  height: 100%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}

.popup-content .main {
  flex: 1;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-top: 36rpx;
  padding: 24rpx 20rpx;
  overflow-y: auto;
}

.popup-content .main .textarea {
  width: 100%;
  border: 1px solid #D4D4D4;
  height: 184rpx;
  border-radius: 4rpx;
  padding: 24rpx 28rpx;
  box-sizing: border-box;
  font-size: 32rpx;
}

.popup-content .main .textarea textarea {
  width: 100%;
  height: 100%;
}

.popup-content .main .time {
  display: flex;
  align-items: center;
  margin-bottom: 56rpx;
}

.popup-content .main .time .name {
  flex-shrink: 0;
  color: #111111;
  font-size: 32rpx;
  font-weight: 700;
  margin-right: 68rpx;
}

.popup-content .main .time .value {
  border: 1px solid #D4D4D4;
  background: #FFFFFF;
  width: 456rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 4rpx;
  font-size: 30rpx;
  color: #111111;
  padding-left: 64rpx;
}

.popup-content .main .header,
.list-item {
  display: flex;
  margin-bottom: 20rpx;
}

.popup-content .main .header .item {
  font-size: 32rpx;
  color: #111111;
  font-weight: 700;
}

.popup-content .main .list-item .item {
  display: flex;
  font-size: 32rpx;
}

.popup-content .main .operation {
  height: 60rpx;
  width: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #CCCCCC;
  color: #B2B2B2;
  box-sizing: border-box;
}

.popup-content .main .list-item .item input {
  width: 92rpx;
  border: 1px solid #CCCCCC;
  box-sizing: border-box;
  background: #FFFFFF;
  height: 30px;
  margin: 0 8rpx;
  text-align: center;
}

.popup-content .main .add1 {
  display: flex;
  align-items: center;
  color: #32AE57;
  margin-bottom: 40rpx;
}

.popup-content .main .add1 view {
  height: 60rpx;
  width: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #CCCCCC;
  color: #32AE57;
  box-sizing: border-box;
  margin-right: 40rpx;
}

.popup-content .main .add {
  color: #32AE57;
}

.select {
  border: 1px solid #CCCCCC;
  width: 100%;
  border-radius: 2px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 4rpx;
}

.delete {
  font-size: 24rpx;
  display: flex;
  align-items: center;
  margin-left: 4rpx;
}
.tips {
  display: flex;
  align-items: center;
  margin-left: 10rpx;
}
.tips image {
  width: 32rpx;
  height: 32rpx;
}

.nodata {
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;
}
.delay-list{
  margin: 10px 14px;
}
.delay-list .delay{
  margin-bottom: 10px;
  display: flex;
  align-items: center;
}
.delay-list .delay .green{
  width: 3px;
  height: 16px;
  border-radius: 1.5px;
  opacity: 1;
  background: #32AE57;
  display: inline-block;
  margin-right: 8px;
}
.delay-list .items{
  display: flex;
  padding: 14px 18px;
  align-items: center;
  font-family: PingFang SC;
  font-size: 15px;
  font-weight: 500;
  line-height: 19px;
  letter-spacing: 0.2px;
  font-variation-settings: "opsz" auto;
  color: #111111;
  border-radius: 8px;
  opacity: 1;
  background: #FFFFFF;
  box-shadow: 0px 2px 6px 0px rgba(26, 33, 44, 0.08);
  margin-bottom: 10px;
}
.delay-list .items .content .name{
  margin-right: 22px;
}
.delay-list .items .time{
  margin-left: auto;
  font-family: Arial;
  font-size: 15px;
  font-weight: normal;
  line-height: 17px;
  letter-spacing: 0.2px;
  color: #666666;
}
.delay-list .items image {
  flex-shrink: 0;
  width: 36rpx;
  height: 36rpx;
  margin-right: 32rpx;
  z-index: 10;
}

.bottom{
  left: 10px;
  right: 10px;
  position: absolute;
  bottom: 23px;
}
.bottom .btn-delay{
  width: 100%;
  height: 34px;
  color: #fff;
  background: #32AE57;
  line-height: 34px;
  border-radius: 17px;
  text-align: center;
}
.delay-content{
  box-sizing: border-box;
  /* padding: 0 14px; */
}

.delay-content .title {
  margin-top: 12px;
  text-align: center;
  font-size: 20px;
  font-weight: 700;
  line-height: 22px;
  letter-spacing: 0.2px;
  color: #111111;
}
.content-main {
  display: flex;
  background: #FFFFFF;
  border-radius: 16rpx;
  /* margin-top: 36rpx; */
  padding: 48rpx 160rpx;
}

.content-main .operation {
  height: 60rpx;
  width: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #CCCCCC;
  color: #B2B2B2;
  box-sizing: border-box;
}
.content-main input{
  width: 92rpx;
  height: 60rpx;
  margin: 0 16rpx;
  border: 1px solid #CCCCCC;
  text-align: center;
}
.content-main .add {
  color: #32AE57;
}
.delay-content-footer {
  box-sizing: border-box;
  display: flex;
  /* height: 58px; */
  padding: 7px 0;
  background: #FFFFFF;
  width: 100%;
  margin-top: 10px;
  border-top: 1px solid #e2e2e2;
}

.delay-content-footer .close {
  flex: 1;
  /* height: 42px; */
  line-height: 28px;
  text-align: center;
  box-sizing: border-box;
  border-right: 1px solid #e2e2e2;
  /* border-radius: 100px; */
  color: #666666;
  margin-right: 13px;
  font-size: 16px;
}

.delay-content-footer .confirm {
  flex: 1;
  /* height: 42px; */
  line-height: 28px;
  text-align: center;
  box-sizing: border-box;
  /* background: linear-gradient(123deg, #FF9035 28%, #FF7B35 73%); */
  /* border-radius: 100px; */
  /* color: #FFFFFF; */
  font-size: 16px;
  color: #00982D;
}